pkgname=git-gone
pkgver=1.0.0
pkgrel=1
arch=('i686' 'x86_64')
license=('MIT')
url="https://github.com/eed3si9n/git-gone"
source=("git-gone-repo::git+https://github.com/eed3si9n/git-gone")
sha1sums=('SKIP')

package() {
  mkdir -p "${pkgdir}/usr/bin"
  cp "${srcdir}/git-gone/git-gone" "${pkgdir}/usr/bin"
}

